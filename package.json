{
  "name": "the-log",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "homepage": "./",
  "dependencies": {
    "immutable": "^3.8.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-scripts-ts": "2.17.0",
    "slate": "^0.40.2",
